#!/bin/bash

# Store the current working directory in a variable
current_dir=$(pwd)

# Change to a new directory
cd /tmp/my_second_directory

# Perform some operation in the new directory
echo "Working in /tmp/my_second_directory"

# Change back to the previous directory
cd "$current_dir"

# Verify that the current working directory is the same as before
if [ "$(pwd)" == "$current_dir" ]; then
    echo "Working directory changed back to $current_dir"
else
    echo "Failed to change back to $current_dir"
fi
